    "@mantine/form": "^5.1.6",
    "@mantine/hooks": "^5.1.6",
    "@mantine/modals": "^5.1.6",
    "@mantine/notifications": "^5.1.6",
    "@mantine/nprogress": "^5.1.6",
    "@mantine/remix": "^5.1.6",
    "@mantine/spotlight": "^5.1.6",
    "@remix-run/node": "^1.6.8",
    "@remix-run/react": "^1.6.8",
    "@remix-run/serve": "^1.6.8",
    "@tabler/icons": "^1.83.1",
    "dayjs": "^1.11.5",
